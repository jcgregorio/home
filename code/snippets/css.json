{
  "grid": {
    "prefix": "grid",
    "body": [
      ".$1 {",
      "\tgrid-area: $2;",
      "}",
      "",
      ".$3 {",
      "\tgrid-area: $4;",
      "}",
      "",
      ".container {",
      "  display: grid;",
      "\tgrid-template-columns: 2fr 1fr;",
      "\tgrid-template-rows: 50px 1fr;",
      "\tgrid-template-areas:",
      "\t\t\"$1 $1\"",
      "\t\t\"$2 $2\"",
      "}"
    ],
    "description": "cssgrid"
  }
}